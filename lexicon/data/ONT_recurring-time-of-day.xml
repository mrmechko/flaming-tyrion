<!DOCTYPE ONTTYPE SYSTEM "onttype.dtd">
<ONTTYPE name="recurring-time-of-day" parent="time-interval" modified="Thu Jun 11 01:45:08 2020" comment="recurring moments of the day, defined by some event">
<SEM fltype="time">
<FEATURES type="recurring-time-of-day" time-function="day-point" tangible="+" scale="time-loc-scale" />
</SEM>
<ARGUMENT role="extent" optionality="optional" fltype="abstr-obj">
<FEATURES scale="duration-scale" />
</ARGUMENT>
<ARGUMENT role="figure" optionality="optional" fltype="(? t533 situation abstr-obj time)" />
<ARGUMENT role="ground" optionality="optional" fltype="time">
<FEATURES time-function="time-frame" scale="duration-scale" />
</ARGUMENT>
