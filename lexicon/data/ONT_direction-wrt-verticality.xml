<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../style/onttype.xsl"?>
<!DOCTYPE ONTTYPE SYSTEM "onttype.dtd">
<ONTTYPE name="direction-wrt-verticality" parent="direction" modified="Fri Apr 26 09:16:51 2019" comment="direction wrt respect to verticality (e.g., gravity or a 'vertical' scale, e.g., temperature), possibly also related to a extended object, e.g., a street, a body, which is the ground">
<SEM fltype="abstr-obj">
<FEATURES type="direction-wrt-verticality" />
</SEM>
<ARGUMENT role="norole" optionality="optional" fltype="?!type" />
<ARGUMENT role="ground" optionality="essential" fltype="phys-obj">
<FEATURES scale="(? !sc473 time-measure-scale)" />
</ARGUMENT>
<ARGUMENT role="figure" optionality="essential" fltype="(? t50417 phys-obj abstr-obj situation)">
<FEATURES aspect="dynamic" mobility="movable" />
</ARGUMENT>
<CHILD name="direction-down" />
<CHILD name="direction-down-ground" />
<CHILD name="direction-up" />
<CHILD name="direction-up-ground" />
</ONTTYPE>
