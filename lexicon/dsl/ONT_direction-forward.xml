<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="ont-type.xsl"?>
<dsl>
 <provenance name="TRIPS"/>
 <concept name="ont::direction-forward">
 <comment>
  &quot;FIGURE moves forward from its original position, where forward is defined by the orientation of the GROUND&quot;
 </comment>
 <relation label="inherit">
  ont::direction-wrt-entity
 </relation>
 <relation label="overlap">
  wn::|forward%4:02:00::|
 </relation>
 </concept>
 <concept name="ont::root" />
 <concept name="abstr-obj" />
 <concept name="ont::referential-sem">
 <comment>
  &quot;The root type for all things that can be referred to: abstract meaning for THIS and IT&quot;
 </comment>
 <relation label="inherit">
  ont::any-sem
 </relation>
 <relation label="overlap">
  wn::|entity%1:03:00::|
 </relation>
 <or>
  <concept name="phys-obj" />
  <concept name="abstr-obj" />
  <concept name="situation" />
  <concept name="proposition" />
 </or>
 </concept>
 <concept name="ont::direction-wrt-entity">
 <comment>
  &quot;direction wrt respect to a given entity (the GROUND), i.e., the relation between the FIGUREs orginal and final position is defined with respect to the GROUND&quot;
 </comment>
 <relation label="inherit">
  ont::direction
 </relation>
 </concept>
 <concept name="ont::direction">
 <comment>
  &quot;a direction = a spatial relation between the location (FIGURE) of an object and its previous location&quot;
 </comment>
 <relation label="inherit">
  ont::position-reln
 </relation>
 <sem-frame>
  <role-restr-map roles="ont::ground">
   <concept name="phys-obj" />
  </role-restr-map>
  <role-restr-map roles="ont::figure">
   <sem-feats>
    <or>
     <concept name="phys-obj" />
     <concept name="abstr-obj" />
     <concept name="situation" />
    </or>
    <feat name="mobility">movable</feat>
   </sem-feats>
  </role-restr-map>
 </sem-frame>
 </concept>
 <concept name="ont::position-reln">
 <comment>
  &quot;Spatial relations that locate one object (the figure) in terms of another object (the ground), which often is implicit&quot;
 </comment>
 <relation label="inherit">
  ont::abstract-object
 </relation>
 <sem-frame>
  <role-restr-map roles="ont::ground">
   <or>
    <concept name="phys-obj" />
    <concept name="situation" />
    <concept name="abstr-obj" />
   </or>
  </role-restr-map>
  <role-restr-map roles="ont::figure">
   <or>
    <concept name="phys-obj" />
    <concept name="situation" />
    <concept name="abstr-obj" />
   </or>
  </role-restr-map>
 </sem-frame>
 </concept>
 <sem-frame>
 <role-restr-map roles="ont::ground">
  <concept name="phys-obj" />
 </role-restr-map>
 <role-restr-map roles="ont::figure">
  <sem-feats>
   <or>
    <concept name="phys-obj" />
    <concept name="abstr-obj" />
    <concept name="situation" />
   </or>
   <feat name="mobility">movable</feat>
  </sem-feats>
 </role-restr-map>
 </sem-frame>
 <concept name="ont::abstract-object">
 <comment>
  &quot;abstract objects, mental constructions, with no physical realization&quot;
 </comment>
 <relation label="overlap">
  wn::|psychological_feature%1:03:00::|
  wn::|abstraction%1:03:00::|
  wn::|abstract_entity%1:03:00::|
 </relation>
 <relation label="inherit">
  ont::referential-sem
  abstr-obj
 </relation>
 </concept>
 <sem-frame>
 <role-restr-map roles="ont::ground">
  <or>
   <concept name="phys-obj" />
   <concept name="situation" />
   <concept name="abstr-obj" />
  </or>
 </role-restr-map>
 <role-restr-map roles="ont::figure">
  <or>
   <concept name="phys-obj" />
   <concept name="situation" />
   <concept name="abstr-obj" />
  </or>
 </role-restr-map>
 </sem-frame>
 <concept name="ont::any-sem">
 <relation label="inherit">
  ont::root
 </relation>
 </concept>
 <or>
 <concept name="phys-obj" />
 <concept name="abstr-obj" />
 <concept name="situation" />
 <concept name="proposition" />
 </or>
 <sense>
 <morph>
  <pos pos="adv" />
  <word first-word="forward" />
 </morph>
 <relation label="inherit">
  ont::direction-forward
  ont::pred-s-post-templ
 </relation>
 </sense>
 <sense>
 <morph>
  <pos pos="adv" />
  <word first-word="forwards" />
 </morph>
 <relation label="inherit">
  ont::direction-forward
  ont::pred-s-post-templ
 </relation>
 </sense>
</dsl>
