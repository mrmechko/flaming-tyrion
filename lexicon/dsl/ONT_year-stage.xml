 <role-restr-map roles="ont::figure" optional="optional">
  <concept name="t" />
 </role-restr-map>
 </sem-frame>
 <sem-feats>
 <relation label="inherit">
  time
 </relation>
 <feat name="time-scale">
 <or>
  point
  interval
 </or>
 </feat>
 </sem-feats>
 <concept name="ont::any-time-object">
 <relation label="inherit">
  ont::any-sem
 </relation>
 <sem-feats>
  <relation label="inherit">
   time
  </relation>
  <feat name="time-scale">
  <or>
   interval
   point
  </or>
  </feat>
  <feat name="time-function">any-time-function</feat>
 </sem-feats>
 </concept>
 <concept name="ont::time-object">
