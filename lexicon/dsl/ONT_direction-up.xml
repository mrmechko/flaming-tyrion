<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="ont-type.xsl"?>
<dsl>
 <provenance name="TRIPS"/>
 <concept name="ont::direction-up">
 <comment>
  &quot;This is the intransitive 'up' and is relative to some scale/domain: e.g., the temperature is up, move the ball up&quot;
 </comment>
 <relation label="inherit">
  ont::direction
 </relation>
 </concept>
 <concept name="ont::root" />
 <or>
 <concept name="phys-obj" />
 <concept name="abstr-obj" />
 <concept name="situation" />
 <concept name="proposition" />
 </or>
 <concept name="ont::any-sem">
 <relation label="inherit">
  ont::root
 </relation>
 </concept>
 <concept name="abstr-obj" />
 <concept name="ont::referential-sem">
 <comment>
  &quot;The root type for all things that can be referred to: abstract meaning for THIS and IT&quot;
 </comment>
 <relation label="inherit">
  ont::any-sem
 </relation>
 <relation label="overlap">
  wn::|entity%1:03:00::|
 </relation>
 <or>
  <concept name="phys-obj" />
  <concept name="abstr-obj" />
  <concept name="situation" />
  <concept name="proposition" />
 </or>
 </concept>
 <sem-frame>
 <role-restr-map roles="ont::ground">
  <or>
   <concept name="phys-obj" />
   <concept name="situation" />
   <concept name="abstr-obj" />
  </or>
 </role-restr-map>
 <role-restr-map roles="ont::figure">
  <or>
   <concept name="phys-obj" />
   <concept name="situation" />
   <concept name="abstr-obj" />
  </or>
 </role-restr-map>
 </sem-frame>
 <concept name="ont::abstract-object">
 <comment>
  &quot;abstract objects, mental constructions, with no physical realization&quot;
 </comment>
 <relation label="overlap">
  wn::|psychological_feature%1:03:00::|
  wn::|abstraction%1:03:00::|
  wn::|abstract_entity%1:03:00::|
 </relation>
 <relation label="inherit">
  ont::referential-sem
  abstr-obj
 </relation>
 </concept>
 <sem-frame>
 <role-restr-map roles="ont::ground">
  <concept name="phys-obj" />
 </role-restr-map>
 <role-restr-map roles="ont::figure">
  <or>
   <concept name="phys-obj" />
   <concept name="abstr-obj" />
   <concept name="situation" />
  </or>
 </role-restr-map>
 </sem-frame>
 <concept name="ont::position-reln">
 <comment>
  &quot;Spatial relations that locate one object (the figure) in terms of another object (the ground)&quot;
 </comment>
 <relation label="inherit">
  ont::abstract-object
 </relation>
 <sem-frame>
  <role-restr-map roles="ont::ground">
   <or>
    <concept name="phys-obj" />
    <concept name="situation" />
    <concept name="abstr-obj" />
   </or>
  </role-restr-map>
  <role-restr-map roles="ont::figure">
   <or>
    <concept name="phys-obj" />
    <concept name="situation" />
    <concept name="abstr-obj" />
   </or>
  </role-restr-map>
 </sem-frame>
 </concept>
 <concept name="ont::direction">
 <relation label="inherit">
  ont::position-reln
 </relation>
 <sem-frame>
  <role-restr-map roles="ont::ground">
   <concept name="phys-obj" />
  </role-restr-map>
  <role-restr-map roles="ont::figure">
   <or>
    <concept name="phys-obj" />
    <concept name="abstr-obj" />
    <concept name="situation" />
   </or>
  </role-restr-map>
 </sem-frame>
 </concept>
 <sense>
 <morph>
  <pos pos="adv" />
  <word first-word="up" />
 </morph>
 <provenance name="TRIPS">
  <provenance name="BASE500"/>
 </provenance>
 <example text="pan the camera UP">
 </example>
 <relation label="inherit">
  ont::direction-up
  ont::particle-templ
 </relation>
 </sense>
 <sense>
 <morph>
  <pos pos="adv" />
  <word first-word="up" />
 </morph>
 <provenance name="TRIPS">
  <provenance name="BASE500"/>
 </provenance>
 <example text="his weight / the temperature is up (X)">
 </example>
 <relation label="inherit">
  ont::direction-up
  ont::predicative-only-adj-templ
 </relation>
 </sense>
</dsl>
