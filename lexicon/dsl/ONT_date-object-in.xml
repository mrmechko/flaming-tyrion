<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="ont-type.xsl"?>
<dsl>
 <provenance name="TRIPS"/>
 <concept name="ont::date-object-in">
 <comment>
  &quot;temporal objects that use IN - e.g., in June&quot;
 </comment>
 <relation label="inherit">
  ont::time-object
 </relation>
 </concept>
 <concept name="time" />
 <concept name="ont::root" />
 <concept name="ont::time-object">
 <comment>
  &quot;objects that refer to temporal locations in some way&quot;
 </comment>
 <relation label="inherit">
  ont::any-time-object
 </relation>
 <sem-feats>
  <relation label="inherit">
   time
  </relation>
  <feat name="time-scale">
  <or>
   point
   interval
  </or>
  </feat>
  <feat name="type">time-object</feat>
 </sem-feats>
 <sem-frame>
  <role-restr-map roles="ont::figure" optional="optional">
   <concept name="t" />
  </role-restr-map>
 </sem-frame>
 </concept>
 <concept name="ont::any-time-object">
 <relation label="inherit">
  ont::any-sem
 </relation>
 <sem-feats>
  <relation label="inherit">
   time
  </relation>
  <feat name="time-scale">
  <or>
   interval
   point
  </or>
  </feat>
  <feat name="time-function">any-time-function</feat>
  <feat name="type">any-time-object</feat>
 </sem-feats>
 </concept>
 <sem-feats>
 <relation label="inherit">
  time
 </relation>
 <feat name="time-scale">
 <or>
  point
  interval
 </or>
 </feat>
 <feat name="type">time-object</feat>
 </sem-feats>
 <sem-frame>
 <role-restr-map roles="ont::figure" optional="optional">
  <concept name="t" />
 </role-restr-map>
 </sem-frame>
 <concept name="ont::any-sem">
 <relation label="inherit">
  ont::root
 </relation>
 </concept>
 <sem-feats>
 <relation label="inherit">
  time
 </relation>
 <feat name="time-scale">
 <or>
  interval
  point
 </or>
 </feat>
 <feat name="time-function">any-time-function</feat>
 <feat name="type">any-time-object</feat>
 </sem-feats>
 <concept name="ont::time-span">
 <relation label="inherit">
  ont::date-object-in
 </relation>
 <sem-frame>
  <role-restr-map roles="ont::figure">
   <concept name="t" />
  </role-restr-map>
 </sem-frame>
 </concept>
 <concept name="ont::era">
 <relation label="inherit">
  ont::date-object-in
 </relation>
 <relation label="overlap">
  wn::|era%1:28:00::|
  wn::|era%1:28:01::|
 </relation>
 <sem-feats>
  <relation label="inherit">
   time
  </relation>
  <feat name="time-function">era</feat>
  <feat name="type">era</feat>
 </sem-feats>
 </concept>
 <concept name="ont::year">
 <relation label="inherit">
  ont::date-object-in
 </relation>
 <relation label="overlap">
  wn::|year%1:28:00::|
  wn::|year%1:28:01::|
  wn::|year%1:28:02::|
 </relation>
 <sem-feats>
  <relation label="inherit">
   time
  </relation>
  <feat name="time-function">year-name</feat>
  <feat name="type">year</feat>
 </sem-feats>
 </concept>
 <concept name="ont::century">
 <relation label="inherit">
  ont::date-object-in
 </relation>
 <relation label="overlap">
  wn::|century%1:28:00::|
 </relation>
 <sem-feats>
  <relation label="inherit">
   time
  </relation>
  <feat name="time-function">time-of-year</feat>
  <feat name="type">century</feat>
 </sem-feats>
 </concept>
 <concept name="ont::day-stage">
 <comment>
  &quot;a regular part of the day&quot;
 </comment>
 <relation label="inherit">
  ont::date-object-in
 </relation>
 <relation label="overlap">
  wn::|morning%1:28:00::|
  wn::|evening%1:28:00::|
  wn::|night%1:28:00::|
  wn::|twilight%1:28:00::|
  wn::|afternoon%1:28:00::|
 </relation>
 <sem-feats>
  <relation label="inherit">
   time
  </relation>
  <feat name="time-function">day-period</feat>
  <feat name="type">day-stage</feat>
 </sem-feats>
 </concept>
 <concept name="ont::year-stage">
 <relation label="inherit">
  ont::date-object-in
 </relation>
 <sem-feats>
  <relation label="inherit">
   time
  </relation>
  <feat name="time-function">time-of-year</feat>
  <feat name="type">year-stage</feat>
 </sem-feats>
 </concept>
</dsl>
