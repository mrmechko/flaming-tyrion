<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="ont-type.xsl"?>
<dsl>
 <provenance name="TRIPS"/>
 <concept name="ont::date-object-in">
 <comment>
  &quot;date objects that use IN - e.g., in June&quot;
 </comment>
 <relation label="inherit">
  ont::time-object
 </relation>
 </concept>
 <concept name="time" />
 <concept name="ont::root" />
 <sem-feats>
 <relation label="inherit">
  time
 </relation>
 <feat name="time-scale">
 <or>
  interval
  point
 </or>
 </feat>
 <feat name="time-function">any-time-function</feat>
 </sem-feats>
 <concept name="ont::any-sem">
 <relation label="inherit">
  ont::root
 </relation>
 </concept>
 <sem-feats>
 <relation label="inherit">
  time
 </relation>
 <feat name="time-scale">
 <or>
  point
  interval
 </or>
 </feat>
 </sem-feats>
 <concept name="ont::any-time-object">
 <relation label="inherit">
  ont::any-sem
 </relation>
 <sem-feats>
  <relation label="inherit">
   time
  </relation>
  <feat name="time-scale">
  <or>
   interval
   point
  </or>
  </feat>
  <feat name="time-function">any-time-function</feat>
 </sem-feats>
 </concept>
 <concept name="ont::time-object">
 <relation label="inherit">
  ont::any-time-object
 </relation>
 <sem-feats>
  <relation label="inherit">
   time
  </relation>
  <feat name="time-scale">
  <or>
   point
   interval
  </or>
  </feat>
 </sem-feats>
 </concept>
 <concept name="ont::month-name">
 <relation label="inherit">
  ont::date-object-in
 </relation>
 <relation label="overlap">
  wn::|calendar_month%1:28:00::|
  wn::|month%1:28:01::|
 </relation>
 <sem-feats>
  <relation label="inherit">
   time
  </relation>
  <feat name="time-function">month-name</feat>
 </sem-feats>
 </concept>
 <concept name="ont::era">
 <relation label="inherit">
  ont::date-object-in
 </relation>
 <sem-feats>
  <relation label="inherit">
   time
  </relation>
  <feat name="time-function">era</feat>
 </sem-feats>
 </concept>
</dsl>
