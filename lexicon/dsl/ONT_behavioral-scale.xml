<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="ont-type.xsl"?>
<dsl>
 <provenance name="TRIPS"/>
 <concept name="ont::behavioral-scale">
 <comment>
  &quot;scales relating to behavioral or psychological attributes that characterize an entity. Property-val counterparts in this scale include both psychological-property-val and animal-propensity-val. To be distinguished from the temporary conditions (stage-scale).&quot;
 </comment>
 <relation label="inherit">
  ont::attributive-scale
 </relation>
 </concept>
 <or>
 <concept name="phys-obj" />
 <concept name="abstr-obj" />
 <concept name="situation" />
 </or>
 <concept name="ont::root" />
 <sem-feats>
 <relation label="inherit">
  abstr-obj
 </relation>
 <feat name="type">ont::abstract-object</feat>
 </sem-feats>
 <concept name="ont::referential-sem">
 <comment>
  &quot;The root type for all things that can be referred to: abstract meaning for THIS and IT&quot;
 </comment>
 <relation label="inherit">
  ont::any-sem
 </relation>
 <relation label="overlap">
  wn::|entity%1:03:00::|
 </relation>
 <sem-feats>
  <or>
   <concept name="phys-obj" />
   <concept name="abstr-obj" />
   <concept name="situation" />
  </or>
  <feat name="type">ont::referential-sem</feat>
 </sem-feats>
 </concept>
 <sem-frame>
 <role-restr-map roles="ont::extent" optional="optional">
  <concept name="t" />
 </role-restr-map>
 <role-restr-map roles="ont::ground" optional="optional">
  <concept name="t" />
 </role-restr-map>
 <role-restr-map roles="ont::figure">
  <concept name="t" />
 </role-restr-map>
 </sem-frame>
 <concept name="ont::tangible-abstract-object">
 <comment>
  &quot;abstract notions that act like things. They can be created, transfered, e.g., mental objects&quot;
 </comment>
 <relation label="inherit">
  ont::abstract-object
 </relation>
 <sem-feats>
  <relation label="inherit">
   abstr-obj
  </relation>
  <feat name="tangible">+</feat>
  <feat name="type">ont::tangible-abstract-object</feat>
 </sem-feats>
 </concept>
 <concept name="ont::attributive-scale">
 <comment>
  &quot;scales dealing with quality or property of something or someone(e.g. smoothness or kindness) and are (relatively) permanent in nature. These are distinguished from ont::stage-scale types that describes scales dealing with temporary stages or states in which an entity is found (a state of being; e.g. healthiness or sleepiness).&quot;
 </comment>
 <relation label="inherit">
  ont::ordered-domain
 </relation>
 <relation label="overlap">
  wn::|quality%1:07:00::|
 </relation>
 </concept>
 <concept name="ont::ordered-domain">
 <relation label="inherit">
  ont::domain
 </relation>
 </concept>
 <concept name="ont::domain">
 <comment>
  &quot;Nouns that name domain/scales, and can serve as relational nouns (e.g., the COLOR of the box)&quot;
 </comment>
 <relation label="inherit">
  ont::tangible-abstract-object
 </relation>
 <relation label="overlap">
  wn::|attribute%1:03:00::|
 </relation>
 <sem-frame>
  <role-restr-map roles="ont::extent" optional="optional">
   <concept name="t" />
  </role-restr-map>
  <role-restr-map roles="ont::ground" optional="optional">
   <concept name="t" />
  </role-restr-map>
  <role-restr-map roles="ont::figure">
   <concept name="t" />
  </role-restr-map>
 </sem-frame>
 </concept>
 <concept name="ont::abstract-object">
 <comment>
  &quot;abstract objects, mental constructions, with no physical realization&quot;
 </comment>
 <relation label="inherit">
  ont::referential-sem
 </relation>
 <relation label="overlap">
  wn::|psychological_feature%1:03:00::|
  wn::|abstraction%1:03:00::|
  wn::|abstract_entity%1:03:00::|
 </relation>
 <sem-feats>
  <relation label="inherit">
   abstr-obj
  </relation>
  <feat name="type">ont::abstract-object</feat>
 </sem-feats>
 </concept>
 <sem-feats>
 <relation label="inherit">
  abstr-obj
 </relation>
 <feat name="tangible">+</feat>
 <feat name="type">ont::tangible-abstract-object</feat>
 </sem-feats>
 <concept name="ont::any-sem">
 <relation label="inherit">
  ont::root
 </relation>
 </concept>
 <sem-feats>
 <or>
  <concept name="phys-obj" />
