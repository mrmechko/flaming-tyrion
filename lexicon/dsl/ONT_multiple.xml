<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="ont-type.xsl"?>
<dsl>
 <provenance name="TRIPS"/>
 <concept name="ont::multiple">
 <relation label="inherit">
  ont::mathematical-term
  abstr-obj
 </relation>
 </concept>
 <concept name="ont::root" />
 <or>
 <concept name="phys-obj" />
 <concept name="abstr-obj" />
 <concept name="situation" />
 <concept name="proposition" />
 </or>
 <concept name="ont::any-sem">
 <relation label="inherit">
  ont::root
 </relation>
 </concept>
 <concept name="ont::referential-sem">
 <comment>
  &quot;The root type for all things that can be referred to: abstract meaning for THIS and IT&quot;
 </comment>
 <relation label="inherit">
  ont::any-sem
 </relation>
 <relation label="overlap">
  wn::|entity%1:03:00::|
 </relation>
 <or>
  <concept name="phys-obj" />
  <concept name="abstr-obj" />
  <concept name="situation" />
  <concept name="proposition" />
 </or>
 </concept>
 <concept name="ont::abstract-object">
 <comment>
  &quot;abstract objects, mental constructions, with no physical realization&quot;
 </comment>
 <relation label="overlap">
  wn::|psychological_feature%1:03:00::|
  wn::|abstraction%1:03:00::|
  wn::|abstract_entity%1:03:00::|
 </relation>
 <relation label="inherit">
  ont::referential-sem
  abstr-obj
 </relation>
 </concept>
 <sem-frame>
 <role-restr-map roles="ont::ground">
  <sem-feats>
   <relation label="inherit">
    abstr-obj
   </relation>
   <feat name="measure-function">value</feat>
  </sem-feats>
 </role-restr-map>
 <role-restr-map roles="ont::figure">
  <sem-feats>
   <relation label="inherit">
    abstr-obj
   </relation>
   <feat name="measure-function">term</feat>
  </sem-feats>
 </role-restr-map>
 </sem-frame>
 <sem-feats>
 <relation label="inherit">
  abstr-obj
 </relation>
 <feat name="information">data</feat>
 <feat name="gradability">-</feat>
 </sem-feats>
 <concept name="ont::abstract-object-nontemporal">
 <comment>
  &quot;formal constructions with no temporal existence: e.g., facts, types, ...&quot;
 </comment>
 <relation label="inherit">
  ont::abstract-object
  abstr-obj
 </relation>
 </concept>
 <concept name="abstr-obj" />
 <concept name="ont::mathematical-term">
 <relation label="inherit">
  ont::abstract-object-nontemporal
 </relation>
 <sem-feats>
  <relation label="inherit">
   abstr-obj
  </relation>
  <feat name="information">data</feat>
  <feat name="gradability">-</feat>
 </sem-feats>
 <sem-frame>
  <role-restr-map roles="ont::ground">
   <sem-feats>
    <relation label="inherit">
     abstr-obj
    </relation>
    <feat name="measure-function">value</feat>
   </sem-feats>
  </role-restr-map>
  <role-restr-map roles="ont::figure">
   <sem-feats>
    <relation label="inherit">
     abstr-obj
    </relation>
    <feat name="measure-function">term</feat>
   </sem-feats>
  </role-restr-map>
 </sem-frame>
 </concept>
 <sense>
 <morph>
  <pos pos="n" />
  <word first-word="fold" />
  <forms>
   -S-3P
   <plur>
    <word first-word="fold" />
   </plur>
  </forms>
 </morph>
 <example text="increase by two fold">
  <provenance name="TRIPS"/>
 </example>
 <relation label="inherit">
  ont::multiple
 </relation>
 <syntax>
  <template-call template="ont::substance-unit-templ" />
  <syn-sem>
   <syn-sem-map syn-arg="argument" syn-cat="pp" head-word="w::of" sem-role="ont::figure"/>
  </syn-sem>
  <syn-feats>
   <feat name="mass">count</feat>
   <feat name="allow-deleted-comp">+</feat>
   <feat name="case">
   <or>
    sub
    obj
   </or>
   </feat>
   <feat name="sort">substance-unit</feat>
   <feat name="agr">
   <or>
    |3s|
    |3p|
   </or>
   </feat>
  </syn-feats>
 </syntax>
 </sense>
</dsl>
