  <syn-sem>
   <syn-sem-map syn-arg="subcat" syn-cat="np" sem-role="ont::ground"/>
   <syn-sem-map syn-arg="argument" syn-cat="np" sem-role="ont::figure"/>
  </syn-sem>
  <syn-feats>
   <feat name="atype">
   <or>
    post
    pre
    pre-vp
   </or>
   </feat>
   <feat name="sort">binary-constraint</feat>
  </syn-feats>
 </syntax>
 </sense>
 <sense>
 <morph>
  <pos pos="adv" />
  <word first-word="in" remaining-words="between" />
 </morph>
 <example text="the triangle is in between the bananas">
  <provenance name="TRIPS"/>
 </example>
 <relation label="inherit">
  ont::between
 </relation>
 <syntax>
  <template-call template="ont::binary-constraint-s-or-np-templ" />
  <syn-sem>
   <syn-sem-map syn-arg="subcat" syn-cat="np" sem-role="ont::ground"/>
   <syn-sem-map syn-arg="argument" syn-cat="(w::or
                                              s
                                              np)" sem-role="ont::figure"/>
  </syn-sem>
  <syn-feats>
   <feat name="atype">
   <or>
    pre
    post
    pre-vp
   </or>
   </feat>
   <feat name="sort">binary-constraint</feat>
  </syn-feats>
 </syntax>
 </sense>
 <sense>
