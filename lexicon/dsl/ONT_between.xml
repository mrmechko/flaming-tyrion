 <morph>
  <pos pos="adv" />
  <word first-word="inter-" />
 </morph>
 <example text="intercellular; interoperable">
  <provenance name="TRIPS"/>
 </example>
 <relation label="inherit">
  ont::between
  ont::adj-operator-prefix-templ
 </relation>
 </sense>
</dsl>
