<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="ont-type.xsl"?>
<dsl>
 <provenance name="TRIPS"/>
 <concept name="ont::between">
 <relation label="inherit">
  ont::complex-ground-reln
 </relation>
 </concept>
 <concept name="ont::root" />
 <concept name="abstr-obj" />
 <concept name="ont::referential-sem">
 <comment>
  &quot;The root type for all things that can be referred to: abstract meaning for THIS and IT&quot;
 </comment>
 <relation label="inherit">
  ont::any-sem
 </relation>
 <relation label="overlap">
  wn::|entity%1:03:00::|
 </relation>
 <or>
  <concept name="phys-obj" />
  <concept name="abstr-obj" />
  <concept name="situation" />
  <concept name="proposition" />
 </or>
 </concept>
 <sem-frame>
 <role-restr-map roles="ont::ground">
  <or>
   <concept name="phys-obj" />
   <concept name="situation" />
   <concept name="abstr-obj" />
  </or>
 </role-restr-map>
 <role-restr-map roles="ont::figure">
  <or>
   <concept name="phys-obj" />
   <concept name="situation" />
   <concept name="abstr-obj" />
  </or>
 </role-restr-map>
 </sem-frame>
 <concept name="ont::abstract-object">
 <comment>
  &quot;abstract objects, mental constructions, with no physical realization&quot;
 </comment>
 <relation label="overlap">
  wn::|psychological_feature%1:03:00::|
  wn::|abstraction%1:03:00::|
  wn::|abstract_entity%1:03:00::|
 </relation>
 <relation label="inherit">
  ont::referential-sem
  abstr-obj
 </relation>
 </concept>
 <concept name="ont::position-reln">
 <comment>
  &quot;Spatial relations that locate one object (the figure) in terms of another object (the ground), which often is implicit&quot;
 </comment>
 <relation label="inherit">
  ont::abstract-object
 </relation>
 <sem-frame>
  <role-restr-map roles="ont::ground">
   <or>
    <concept name="phys-obj" />
    <concept name="situation" />
    <concept name="abstr-obj" />
   </or>
  </role-restr-map>
  <role-restr-map roles="ont::figure">
   <or>
    <concept name="phys-obj" />
    <concept name="situation" />
    <concept name="abstr-obj" />
   </or>
  </role-restr-map>
 </sem-frame>
 </concept>
 <concept name="ont::complex-ground-reln">
 <comment>
  &quot;FIGURE is located wrt a set of objects that comprise the GROUND. e.g., between X and Y, among the participants&quot;
 </comment>
 <relation label="inherit">
  ont::position-reln
 </relation>
 </concept>
 <concept name="ont::any-sem">
 <relation label="inherit">
  ont::root
 </relation>
 </concept>
 <or>
 <concept name="phys-obj" />
 <concept name="abstr-obj" />
 <concept name="situation" />
 <concept name="proposition" />
 </or>
 <sense>
 <morph>
  <pos pos="adv" />
  <word first-word="between" />
 </morph>
 <provenance name="TRIPS">
  <provenance name="BASE500"/>
 </provenance>
 <example text="it is between a rock and a hard place">
 </example>
 <relation label="inherit">
  ont::between
 </relation>
 <syntax>
  <template-call template="ont::binary-constraint-s-templ" />
  <syn-sem>
   <syn-sem-map syn-arg="subcat" syn-cat="np" sem-role="ont::ground"/>
   <syn-sem-map syn-arg="argument" syn-cat="s" sem-role="ont::figure"/>
  </syn-sem>
  <syn-feats>
   <feat name="atype">
   <or>
    pre
    post
   </or>
   </feat>
   <feat name="sort">binary-constraint</feat>
  </syn-feats>
 </syntax>
 </sense>
 <sense>
 <morph>
  <pos pos="adv" />
  <word first-word="between" />
 </morph>
 <comment>
  &quot;PQ0405&quot;
 </comment>
