<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="ont-type.xsl"?>
<dsl>
 <provenance name="TRIPS"/>
 <concept name="ont::pos-wrt-containment-reln">
 <comment>
  &quot;locating an object (typically rthe FIGURE) within an extended object (typically the GROUND)&quot;
 </comment>
 <relation label="inherit">
  ont::position-reln
 </relation>
 </concept>
 <concept name="ont::root" />
 <concept name="abstr-obj" />
 <concept name="ont::referential-sem">
 <comment>
  &quot;The root type for all things that can be referred to: abstract meaning for THIS and IT&quot;
 </comment>
 <relation label="inherit">
  ont::any-sem
 </relation>
 <relation label="overlap">
  wn::|entity%1:03:00::|
 </relation>
 <or>
  <concept name="phys-obj" />
  <concept name="abstr-obj" />
  <concept name="situation" />
  <concept name="proposition" />
 </or>
 </concept>
 <sem-frame>
 <role-restr-map roles="ont::ground">
  <or>
   <concept name="phys-obj" />
   <concept name="situation" />
   <concept name="abstr-obj" />
  </or>
 </role-restr-map>
 <role-restr-map roles="ont::figure">
  <or>
   <concept name="phys-obj" />
   <concept name="situation" />
   <concept name="abstr-obj" />
  </or>
 </role-restr-map>
 </sem-frame>
 <concept name="ont::abstract-object">
 <comment>
  &quot;abstract objects, mental constructions, with no physical realization&quot;
 </comment>
 <relation label="overlap">
  wn::|psychological_feature%1:03:00::|
  wn::|abstraction%1:03:00::|
  wn::|abstract_entity%1:03:00::|
 </relation>
 <relation label="inherit">
  ont::referential-sem
  abstr-obj
 </relation>
 </concept>
 <concept name="ont::position-reln">
 <comment>
  &quot;Spatial relations that locate one object (the figure) in terms of another object (the ground), which often is implicit&quot;
 </comment>
 <relation label="inherit">
  ont::abstract-object
 </relation>
 <sem-frame>
  <role-restr-map roles="ont::ground">
   <or>
    <concept name="phys-obj" />
    <concept name="situation" />
    <concept name="abstr-obj" />
   </or>
  </role-restr-map>
  <role-restr-map roles="ont::figure">
   <or>
    <concept name="phys-obj" />
    <concept name="situation" />
    <concept name="abstr-obj" />
   </or>
  </role-restr-map>
 </sem-frame>
 </concept>
 <concept name="ont::any-sem">
 <relation label="inherit">
  ont::root
 </relation>
 </concept>
 <or>
 <concept name="phys-obj" />
 <concept name="abstr-obj" />
 <concept name="situation" />
 <concept name="proposition" />
 </or>
 <concept name="ont::in-loc">
 <comment>
  &quot;FIGURE is within or inside the GROUND&quot;
 </comment>
 <relation label="inherit">
  ont::pos-wrt-containment-reln
 </relation>
 <sem-frame>
  <role-restr-map roles="ont::ground">
   <sem-feats>
    <or>
     <concept name="phys-obj" />
     <concept name="abstr-obj" />
    </or>
    <feat name="container">+</feat>
    <feat name="intentional">-</feat>
   </sem-feats>
  </role-restr-map>
 </sem-frame>
 </concept>
 <concept name="ont::contain-reln">
 <comment>
  &quot;a kind of Inverse of IN-LOC, but can't be used as a result location&quot;
 </comment>
 <relation label="inherit">
  ont::pos-wrt-containment-reln
 </relation>
 <sem-frame>
  <role-restr-map roles="ont::figure">
   <sem-feats>
    <relation label="inherit">
     phys-obj
    </relation>
    <feat name="container">+</feat>
    <feat name="intentional">-</feat>
   </sem-feats>
  </role-restr-map>
 </sem-frame>
 </concept>
 <concept name="ont::outside">
 <relation label="inherit">
  ont::pos-wrt-containment-reln
 </relation>
 <sem-frame>
  <role-restr-map roles="ont::ground">
   <sem-feats>
    <relation label="inherit">
     phys-obj
    </relation>
    <feat name="container">+</feat>
    <feat name="intentional">-</feat>
   </sem-feats>
  </role-restr-map>
 </sem-frame>
 </concept>
 <concept name="ont::delimit-reln">
 <comment>
  &quot;the FIGURE has a value in a rnage that is delimited by the GROUND, e.g., within five dollars of the estimate, within five miles of starbucks&quot;
 </comment>
 <relation label="inherit">
  ont::pos-wrt-containment-reln
 </relation>
 <sem-frame>
  <role-restr-map roles="ont::ground">
   <or>
    <concept name="phys-obj" />
    <concept name="abstr-obj" />
   </or>
  </role-restr-map>
  <role-restr-map roles="ont::figure">
   <or>
    <concept name="phys-obj" />
    <concept name="abstr-obj" />
   </or>
  </role-restr-map>
 </sem-frame>
 </concept>
</dsl>
